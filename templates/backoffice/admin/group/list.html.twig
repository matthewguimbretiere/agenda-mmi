<h1>Liste des groupe</h1>
<a href="{{path("back-group-add")}}">Ajouter un groupe <i class="fas fa-plus"></i></i></a>
<ul>
{% for group in groups %}
<li>
<span>Id : {{group.id}}</span>
<span>/ Name :  {{group.name}}</span>
<span>/ Type :  {{group.type}}</span>
<span>/ Semester :  {{group.semester}}</span>
<span>/ Campain :  {{group.campain}}</span>
<a href="{{path("back-group-edit", {"id":group.id})}}">Ã‰diter <i class="fas fa-edit"></i></a>
<a href="{{path("back-group-delete", {"id":group.id})}}">Supprimer <i class="fas fa-trash-alt"></i></a>
</li>
{% endfor %}
</ul>
{% endblock %}