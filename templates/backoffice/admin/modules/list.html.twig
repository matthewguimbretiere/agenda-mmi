{% extends 'backoffice/layout.html.twig' %}
{% block suite_title %}liste des modules{% endblock %}
{% block suite_body %}
<h1>Liste des modules</h1>
<ul>
{% for module in modules %}
<li>
<span>Id :  {{module.id}}</span>
<span>/ Nom :  {{module.name}}</span>
<span>/ Semestre :  {{module.semester}}</span>
<span>/ Campagne :  {{module.campain}}</span>
</span>
</li>
{% endfor %}
</ul>
{% endblock %}